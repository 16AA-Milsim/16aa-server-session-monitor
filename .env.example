DISCORD_TOKEN=
CHANNEL_ID=

# Comma-separated Windows usernames to monitor (case-insensitive).
MONITOR_USERS=16aa,cantina,16aa_public,16aa_testing

# Optional: comma-separated aliases in the form user=Display Name.
USER_ALIASES=cantina=16AA Admin,16aa_public=16AA Public

# Mark "Engaged" when STATE=Active and IDLE <= this.
IDLE_THRESHOLD_MINUTES=10

# Poll `quser` every N seconds.
POLL_SECONDS=15

# Refresh Security event log enrichment every N seconds.
SECURITY_POLL_SECONDS=60

# Geo lookup (best-effort, cached)
GEOLOOKUP_ENABLED=true

